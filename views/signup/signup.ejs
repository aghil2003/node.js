<div id="loginPage">
<div class="login-container">
    <div class="form-box">
      <h2>Login</h2>
      <form action="/signin" method="post">
        <label for="email"><b>email</b></label>
        <input type="text" placeholder="Enter email" name="email">
    
        <label for="password"><b>password</b></label>
        <input type="password" placeholder="Enter password" name="password">
    
        <input type="submit" value="Login" class="btn">
    </form>
        <p class="register-link">
          Don't have an account? <a href="/register">Register here</a>
        </p>
      </form>
    </div>
  </div>
  <div id="login-img-container">
    <img id="login-img1" src="/img/smiley-woman-with-headphones-working-laptop 1.png" alt="Smiley face">
    <img id="login-img2" src="/img/Rectangle 62.png" >
    <img id="login-img3" src="/img/Rectangle 65.png" >
  </div>
</div>
